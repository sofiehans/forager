{
  "expo": {
    "name": "Mushroom Identification",
    "splash": {
      "image": "./assets/images/foragerIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1D3836"
    },
    "slug": "forager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/foragerIcon.png",
    "scheme": "forager",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thechemist1357.forager",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/foragerIcon.png",
        "backgroundColor": "#1D3836"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.forager"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/foragerIcon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1D3836"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d1dd3571-c5e6-4e5f-a249-83d71cad9ea3"
      }
    }
  }
}